{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:44318",
      "sslPort": 44319
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTPS_PORT": "44319",
        "ASPNETCORE_DETAILED_ERRORS": "true"
      }
    },
    "Backend.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:5001;http://+:5000",
        "ASPNETCORE_DETAILED_ERRORS": "true"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "publishAllPorts": true,
      "useSSL": true,
      "sslPort": 443,
      "httpPort": 80,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:443;http://+:80",
        "ASPNETCORE_Kestrel__Certificates__Default__Path": "/app/certs/cert.pfx",
        "DOCKER_REGISTRY": "your-registry.azurecr.io",
        "DOCKER_REPOSITORY": "backend-api",
        "AZURE_SUBSCRIPTION": "your-subscription-id"
      }
    }
  }
}